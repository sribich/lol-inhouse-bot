{
    "name": "inhouse-bot",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "start": "nx serve",
        "build": "nx build",
        "test": "nx test",
        "preinstall": "npx only-allow pnpm",
        "postinstall": "husky install"
    },
    "private": true,
    "dependencies": {
        "@nestjs/common": "8.4.5",
        "@nestjs/core": "8.4.5",
        "@nestjs/platform-express": "8.4.5",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.5",
        "tslib": "2.3.1"
    },
    "devDependencies": {
        "@commitlint/cli": "17.0.2",
        "@commitlint/config-conventional": "17.0.2",
        "@nestjs/schematics": "8.0.11",
        "@nestjs/testing": "8.4.5",
        "@nrwl/cli": "13.10.5",
        "@nrwl/eslint-plugin-nx": "13.10.5",
        "@nrwl/jest": "13.10.5",
        "@nrwl/linter": "13.10.5",
        "@nrwl/nest": "13.10.5",
        "@nrwl/node": "13.10.5",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/workspace": "13.10.5",
        "@types/compression": "1.7.2",
        "@types/jest": "27.0.2",
        "@types/node": "16.11.12",
        "@typescript-eslint/eslint-plugin": "5.18.0",
        "@typescript-eslint/parser": "5.18.0",
        "eslint": "8.12.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "eslint-plugin-sonarjs": "0.13.0",
        "husky": "8.0.1",
        "jest": "27.2.3",
        "lint-staged": "13.0.0",
        "nx": "13.10.5",
        "prettier": "^2.5.1",
        "syncpack": "8.0.0",
        "ts-jest": "27.0.5",
        "typescript": "4.6.4"
    },
    "lint-staged": {
        "{apps,libs,tools}/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "{apps,libs,tools/**/*.{json,md}}": [
            "prettier --write"
        ]
    },
    "synckpac": {
        "dev": true,
        "peer": true,
        "prod": true,
        "filter": ".",
        "indent": "    ",
        "semverRange": "",
        "sortAz": [
            "contributors",
            "dependencies",
            "devDependencies",
            "keywords",
            "peerDependencies",
            "scripts"
        ],
        "sortFirst": [
            "name",
            "description",
            "version",
            "author",
            "contributors",
            "keywords"
        ],
        "source": [
            "package.json",
            "apps/*/package.json",
            "libs/*/package.json",
            "tools/actions/*/package.json"
        ],
        "versionGroups": []
    }
}
