generator client {
    provider = "prisma-client-js"
    output = "../src"
    binaryTargets = ["native", "linux-musl"]
    previewFeatures = ["interactiveTransactions"]
}

datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

model Placeholder {
    id  Int     @id @default(autoincrement())
}